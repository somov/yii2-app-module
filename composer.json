{
  "name": "somov/yii2-app-module",
  "description": "A manage modules Yii2  at run time. Can enable, disable, install, uninstall and update. ",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "module"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Somov",
      "email": "somov.nn@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "ext-zip": "*",
    "ext-mbstring": "*",
    "yiisoft/yii2": ">=2.0.14",
    "somov/yii2-common": ">=1.1.1"
  },

  "autoload-dev": {
    "classmap": [
      "tests/unit/"
    ]
  },

  "require-dev": {
    "codeception/base": "^2.2.3",
    "codeception/verify": "~0.3.1",
    "codeception/specify": "~0.4.3",
    "phpunit/phpunit": "~5"
  },



  "autoload": {
    "psr-4": {
      "somov\\appmodule\\": "src"
    }
  },

  "archive": {
    "exclude": [
      "/.*",
      "/tests",
      "/vendor",
      "/runtime"
    ]
  },

  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  }
}
