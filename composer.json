{
  "name": "somov/yii2-app-module",
  "description": "Manage modules Yii2  at run time. Enable, disable, install, uninstall and update. ",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "module"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Somov",
      "email": "somov.nn@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "ext-zip": "*",
    "yiisoft/yii2": ">=2.0.14",
    "somov/yii2-common": ">=1.1.1"
  },
  "autoload": {
    "psr-4": {
      "somov\\appmodule\\": "src"
    }
  },
  "archive": {
    "exclude": [
      "/.*",
      "/tests",
      "/vendor",
      "/bin"
    ]
  }
}
